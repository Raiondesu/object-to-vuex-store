{
  "name": "object-to-vuex-store",
  "version": "2.4.4",
  "description": "Convert any JS object into a vuex object.",
  "main": "./src/index.js",
  "module": "./dist/index.js",
  "pre-commit": "npm test",
  "scripts": {
    "test": "mocha -R spec test/*-spec.js",
    "build": "uglifyjs ./src/index.js -c toplevel,sequences=false -m reserved=['objectToStore','exports'] --toplevel -o ./dist/index.js",
    "prepublishOnly": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raiondesu/object-to-vuex-store.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "object",
    "convert",
    "plain",
    "JS",
    "javascript",
    "vuejs",
    "store"
  ],
  "author": "Raiondesu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Raiondesu/object-to-vuex-store/issues"
  },
  "homepage": "https://github.com/Raiondesu/object-to-vuex-store#readme",
  "devDependencies": {
    "mocha": "^4.0.1",
    "uglify-js": "^3.1.5",
    "vue": "^2.5.2",
    "vuex": "^3.0.0"
  }
}
